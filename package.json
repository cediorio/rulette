{
    "name": "rulette",
    "version": "0.1.0",
    "description": "a simple javascript rule engine",
    "type": "module",
    "main": "index.js",
    "directories": {
	"test": "test"
    },
    "scripts": {
	"test": "jest",
	"start": "nodemon --exec babel-node ./src/index.js",
	"build": "babel src --out-dir ./lib --source-maps",
	"debug": "nodemon --inspect-brk ./dest/index.js",
	"debug-tests": "node --inspect-brk node_modules/.bin/jest --runInBand"
    },
    "babel": {
	"presets": [
	    [
		"@babel/preset-env",
		{
		    "targets": {
			"node": "current"
		    }
		}
	    ]
	]
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
	"@babel/cli": "^7.7.7",
	"@babel/core": "^7.7.7",
	"@babel/node": "^7.7.7",
	"@babel/preset-env": "^7.7.7",
	"axios-mock-adapter": "^1.17.0",
	"jest": "^24.9.0"
    },
    "dependencies": {
	"babel-jest": "^24.9.0",
	"jest-cli": "^24.9.0",
	"jsep": "^0.3.4",
	"nodemon": "^2.0.2",
	"yarn": "^1.21.1"
    }
}
